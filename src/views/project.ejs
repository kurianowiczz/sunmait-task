<div class="container">
    <div><h1><%= project.name %></h1></div>
    <div class="tasks">
        <p>Your tasks:</p>
        <% allTasks.forEach(function (task) { %>
            <div><a href="/task/<%= task.id %>?token=<%= token %>"> <%= task.name %> - <%= task.description %>  </a></div>
        <% }); %>
    </div>
    <div>
        <a href="/task/<%= project.id %>/add?token=<%= token %>"><button id="add-task">Add task</button></a>
    </div>

</div>
